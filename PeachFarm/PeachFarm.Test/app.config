<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="peachfarm.admin" type="PeachFarm.Admin.Configuration.AdminSection, PeachFarm.Admin" allowLocation="true" allowDefinition="Everywhere" />
		<section name="peachfarm.controller" type="PeachFarm.Controller.Configuration.ControllerSection, PeachFarm.Controller" allowLocation="true" allowDefinition="Everywhere" />
		<section name="peachfarm.node" type="PeachFarm.Node.Configuration.NodeSection, PeachFarm.Node" allowLocation="true" allowDefinition="Everywhere" />
		<section name="peachfarm.reporting" type="PeachFarm.Reporting.Configuration.ReportGeneratorSection, PeachFarm.Reporting" allowLocation="true" allowDefinition="Everywhere"/>
		<section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
		<section
						name="Telerik.Reporting"
						type="Telerik.Reporting.Configuration.ReportingConfigurationSection, Telerik.Reporting, Version=7.0.13.426, Culture=neutral, PublicKeyToken=a9d7983dfcc261be"
						allowLocation="true"
						allowDefinition="Everywhere"/>
	</configSections>
	<peachfarm.admin>
		<Controller ipAddress="localhost" />
		<RabbitMq hostName="localhost" port="-1" userName="guest" password="guest" useSSL="false" />
		<MongoDb connectionString="mongodb://localhost/?safe=true" />
	</peachfarm.admin>
	<peachfarm.controller>
		<Controller nameOverride="localhost" />
		<MongoDb connectionString="mongodb://localhost/?safe=true" />
		<RabbitMq hostName="localhost" port="-1" userName="guest" password="guest" useSSL="false" />
	</peachfarm.controller>
	<peachfarm.node>
		<Controller ipAddress="localhost" />
		<RabbitMq hostName="localhost" port="-1" userName="guest" password="guest" useSSL="false" />
		<Tags>
			<add name="Windows" />
			<add name="x64" />
		</Tags>
	</peachfarm.node>
	<peachfarm.reporting>
		<Controller ipAddress="10.0.1.104" />
		<RabbitMq hostName="10.0.1.104" port="-1" userName="guest" password="guest" useSSL="false" />
		<MongoDb connectionString="mongodb://10.0.1.104/?safe=true" />
		<Monitor baseURL="http://dejaapps/pfmonitor/" />
	</peachfarm.reporting>
	<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

		<!-- make sure to set 'Copy To Output Directory' option for this file -->
		<!-- go to http://nlog-project.org/wiki/Configuration_file for more information -->

		<targets>
			<!--
			<target name="console" xsi:type="Console" layout="${message}"/>
			-->
		</targets>

		<rules>
			<!--
			<logger minlevel="Debug" writeTo="console"/>
			-->
		</rules>
	</nlog>
	<Telerik.Reporting>
		<Cache provider="File" />
	</Telerik.Reporting>
</configuration>