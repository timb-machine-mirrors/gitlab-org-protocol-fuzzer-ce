#!/usr/bin/env python

from waflib import Utils

if 'cshlib' in bld.env['supported_features']:
	dll = bld.env['cshlib_PATTERN'] % '*'
	src = bld.path.find_dir(bld.env['BASENAME']).ant_glob(dll)

	if bld.env.MCS and bld.env.DEST_OS == "win32":
		bld.install_files('${BINDIR}', src, chmod=Utils.O755)
