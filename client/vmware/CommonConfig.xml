		<Define key="TargetVmx" value="[ha-datacenter/datastore1] Target/Target.vmx"/>
		<Define key="TargetVmxHost" value="192.168.2.1"/>
		<Define key="TargetVmxLogin" value="root"/>
		<Define key="TargetVmxPassword" value="vmware123"/>
		<Define key="TargetVmxSnapshotName" value="Fuzzing"/>

		<!-- PortCheck Monitor: Wait for Agent VM -->
		<Define key="PortCheckHost" value="192.168.2.1"/>
		<Define key="PortCheckPort" value="22"/>
