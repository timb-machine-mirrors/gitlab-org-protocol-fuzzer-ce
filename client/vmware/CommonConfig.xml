		<!-- Interface on Agent VM we will send data from -->
		<!-- <Define key="Interface" value="eth1"/> -->
		<Define key="Interface" value="virbr1"/>

		<!-- Fuzzing strategy to use -->
		<Define key="Strategy" value="Random"/>

		<!-- Path to current working folder -->
		<!-- <Define key="Path"     value="/home/pentest/definitions" /> -->
		<Define key="Path"     value="./" />

		<!-- IP Address of remote agent -->
		<Define key="RemoteAgentIP"  value="192.168.2.10"/>

