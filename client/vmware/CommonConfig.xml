		<Define key="Vmx" value="[disk1] ESXi Target/ESXi Target.vmx"/>
		<Define key="VmxHost" value="10.0.1.60"/>
		<Define key="VmxLogin" value="test"/>
		<Define key="VmxPassword" value="vmware123"/>
		<Define key="VmxSnapshotName" value="Fuzzing"/>

		<!-- EsxiConsole Monitor -->
		<Define key="EsxiConsoleSerialPort" value="/dev/ttyS0"/>
		<Define key="EsxiConsoleLog" value="true"/>

