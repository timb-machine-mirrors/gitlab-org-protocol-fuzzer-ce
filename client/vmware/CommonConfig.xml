		<Define key="AgentVmx" value="[ha-datacenter/datastore1] Agent/Agent.vmx"/>
		<Define key="AgentVmxHost" value="192.168.2.1"/>
		<Define key="AgentVmxLogin" value="root"/>
		<Define key="AgentVmxPassword" value="vmware123"/>
		<Define key="AgentVmxSnapshotName" value="Fuzzing"/>

