			<Number name="PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload" />
			</Number>
			<Number name="NextHeader" size="8" value="43"/>
		</Block>
		<Block name="Payload">
			<Block ref="Routing"/>
		</Block>

	</DataModel>

	<DataModel name="IPv6JumbogramPacket">
		<Block ref="EthernetFrame"/>
		<Block ref="IPv6Header">
			<Number name="PayloadLength" size="16" endian="big" value="0000" valueType="hex">
			<!-- <Number name="Header.PayloadLength" size="16" endian="big" value="FFFF" valueType="hex"> -->
				<!-- <Relation type="size" of="Payload"/> -->
			</Number>
			<!-- wireshark doesn't parse this ^ if 0, RFC says should be 0 for payloads over 65535 -->
			<Number name="NextHeader" size="8" value="0"/>
		</Block>
		<Block name="Payload">
			<Block ref="JumboFrame"/>
		</Block>
	</DataModel>

