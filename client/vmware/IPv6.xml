				<Flag name="A" size="1" position="0"/>
				<Flag name="H" size="1" position="1"/>
				<Flag name="L" size="1" position="2"/>
				<Flag name="K" size="1" position="3"/>
				<Flag name="Reserved" size="12" position="4"/>
			</Flags>
