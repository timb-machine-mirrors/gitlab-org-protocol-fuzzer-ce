	<DataModel name="IPv6MobileBAPacket">
		<Block ref="EthernetFrame"/>
		<Block ref="IPv6Header">
			<Number name="PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload"/>
			</Number>
			<Number name="NextHeader" size="8" value="135"/>
		</Block>
		<Block name="Payload">
			<Block ref="Mobile">
				<Number name="HeaderLen" size="8" endian="big">
					<Relation type="size" of="MobilityOptions" expressionGet="(size + 8) * 8" expressionSet="(((size-8) > 0) and ((size-8)/8)) or 1"/>
				</Number>
				<Number name="MH" size="8" endian="big" value="6"/>
				<Number name="Checksum" size="16" endian="big">
					<Fixup class="IcmpV6ChecksumFixup">
						<Param name="ref" value="IPv6MobileBAPacket"/>
						<Param name="src" value="##SourceIPv6##"/>
						<Param name="dst" value="##TargetIPv6##"/>
					</Fixup>
				</Number>
			</Block>
			<Number name="Status" size="8" signed="false"/>
			<Flags size="8" endian="big">
				<Flag name="K" size="1" position="0"/>
				<Flag name="Reserved" size="7" position="1"/>
			</Flags>
			<Number name="Sequnce" size="16" signed="false"/>
			<Number name="Lifetime" size="16" signed="false"/>
			<Block name="MobilityOptions">
				<Block name="BindingRefreshAdvice" ref="TLV">
					<Number name="Type" size="8" endian="big" value="2"/>
					<Number name="Length" size="8" endian="big" value="2"/>
					<Block name="Value">
						<Number name="RefreshInterval" size="16" endian="big"/>
					</Block>
				</Block>
				<Block name="BindingAuthData" ref="TLV">
					<Number name="Type" size="8" endian="big" value="5"/>
					<Number name="Length" size="8" endian="big">
						<Relation type="size" of="Value"/>
					</Number>
					<Block name="Value">
						<Blob name="Authenticator" length="12" valueType="hex"/><!--CHECK IF FIXUP IS NEEDED-->
					</Block>
				</Block>
			</Block>
			<Padding  alignment="64"/>
		</Block>
	</DataModel>

	<DataModel name="IPv6MobileBEPacket">
		<Block ref="EthernetFrame"/>
		<Block ref="IPv6Header">
			<Number name="PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload"/>
			</Number>
			<Number name="NextHeader" size="8" value="135"/>
		</Block>
		<Block name="Payload">
			<Block ref="Mobile">
				<Number name="HeaderLen" size="8" endian="big">
					<Relation type="size" of="MobilityOptions" expressionGet="(size + 8) * 8" expressionSet="(((size-8) > 0) and ((size-8)/8)) or 2"/>
				</Number>
				<Number name="MH" size="8" endian="big" value="7"/>
				<Number name="Checksum" size="16" endian="big">
					<Fixup class="IcmpV6ChecksumFixup">
						<Param name="ref" value="IPv6MobileBEPacket"/>
						<Param name="src" value="##SourceIPv6##"/>
						<Param name="dst" value="##TargetIPv6##"/>
					</Fixup>
				</Number>
			</Block>
			<Number name="Status" size="8" signed="false"/>
			<Number name="Reserved" size="8"/>
			<Blob name="HomeAddress" length="16"/>
			<Block name="MobilityOptions">
			</Block>
			<Padding  alignment="64"/>
		</Block>
	</DataModel>

	<DataModel name="IPv6MobileRoutingType2HeaderPacket">
		<Block ref="EthernetFrame"/>
		<Block ref="IPv6Header">
			<Number name="PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload" />
			</Number>
			<Number name="NextHeader" size="8" value="43"/>
		</Block>
		<Block name="Payload">
			<Block ref="MobileRoutingType2Header">
					<Number name="NextHeader" size="8" endian="big" value="59"/>
			</Block>
		</Block>
	</DataModel>


	<StateModel name="Transmit" initialState="Initial">
		<State name="Initial">
			<Action type="output">
				<DataModel ref="IPv6EmptyPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6HopByHopPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6DestinationPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6RoutingPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6FragmentPacket1"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6FragmentPacket2"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6FragmentPacket3"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileBRRPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileHoTIPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileCoTIPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileHoTPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileCoTPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileBUPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileBAPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileBEPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileRoutingType2HeaderPacket"/>
			</Action>
		</State>
	</StateModel>

	<StateModel name="MtuFuzzStates" initialState="Initial">
		<State name="Initial">
			<Action type="setProperty" property="MTU">
				<DataModel ref="MTU"/>
			</Action>
			<!-- <Action type="output"> -->
