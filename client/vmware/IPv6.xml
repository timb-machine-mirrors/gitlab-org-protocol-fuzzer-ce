	<DataModel name="IPv6MobileBRRPacket">
		<Block ref="EthernetFrame"/>
		<Block ref="IPv6Header">
			<Number name="PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload"/>
			</Number>
			<Number name="NextHeader" size="8" value="135"/>
		</Block>
		<Block name="Payload">
			<Block ref="Mobile">
				<Number name="HeaderLen" size="8" endian="big">
					<Relation type="size" of="MobilityOptions" expressionGet="(size + 8) * 8" expressionSet="(((size-8) > 0) and ((size-8)/8)) or 0"/>
				</Number>
				<Number name="MH" size="8" endian="big" value="0"/>
				<Number name="Checksum" size="16" endian="big">
					<Fixup class="IcmpV6ChecksumFixup">
						<Param name="ref" value="IPv6MobileBRRPacket"/>
						<Param name="src" value="##SourceIPv6##"/>
						<Param name="dst" value="##TargetIPv6##"/>
					</Fixup>
				</Number>
			</Block>
			<Number name="Reserved" size="16"/>
			<Block name="MobilityOptions">
			</Block>
			<Padding  alignment="64"/>
		</Block>
	</DataModel>

