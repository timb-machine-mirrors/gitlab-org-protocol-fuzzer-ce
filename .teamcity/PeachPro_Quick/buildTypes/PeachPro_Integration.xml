  <name>Integration</name>
  <description />
  <settings>
    <parameters />
    <build-runners />
    <vcs-settings />
    <requirements>
      <equals name="teamcity.agent.jvm.os.arch" value="amd64" />
      <contains name="teamcity.agent.jvm.os.name" value="Linux" />
    </requirements>
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:master" />
          <param name="quietPeriodMode" value="USE_DEFAULT" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_1" type="VcsLabeling">
        <parameters>
          <param name="branchFilter" value="+:&lt;default&gt;" />
          <param name="labelingPattern" value="tc-%build.number%" />
          <param name="vcsRootId" value="__ALL__" />
        </parameters>
      </extension>
    </build-extensions>
    <artifact-dependencies>
      <dependency sourceBuildTypeId="PeachPro_Doc" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="archive.zip* =&gt; doc&#xD;&#xA;" />
      </dependency>
      <dependency sourceBuildTypeId="PeachPro_linux_x86_64_debug" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="archive.zip* =&gt; linux_x86_64_debug" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="PeachPro_Doc">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
