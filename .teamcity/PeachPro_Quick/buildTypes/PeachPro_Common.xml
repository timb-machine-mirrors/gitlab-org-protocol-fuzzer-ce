<?xml version="1.0" encoding="UTF-8"?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="cb9b75c1-8ce9-4d77-b330-ea717f7cf405" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>common</name>
  <settings>
    <options>
      <option name="artifactRules" value="output/%waf.variant%/archive.zip&#xA;output/%waf.variant%/archive.zip.sha1" />
      <option name="buildNumberPattern" value="%MajorVersion%.%MinorVersion%.%dep.PeachPro_Root.system.build.number%" />
      <option name="checkoutMode" value="ON_AGENT" />
    </options>
    <parameters>
      <param name="PatchVersion" value="0" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_4" name="configure" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="waf%waf.ext%" />
          <param name="command.parameters" value="configure --strict --verbose --buildtag %build.number%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_5" name="build" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="waf%waf.ext%" />
          <param name="command.parameters" value="build --strict --variant=%waf.variant%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_6" name="install" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="waf%waf.ext%" />
          <param name="command.parameters" value="install --strict --variant=%waf.variant%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_17" name="pkg" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="waf%waf.ext%" />
          <param name="command.parameters" value="pkg --variant=%waf.variant%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_18" name="zip" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="waf%waf.ext%" />
          <param name="command.parameters" value="zip --variant=%waf.variant%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="unittest" type="NUnit">
        <parameters>
          <param name="dotNetCoverage.NCover.HTMLReport.File.Sort" value="0" />
          <param name="dotNetCoverage.NCover.HTMLReport.File.Type" value="1" />
          <param name="dotNetCoverage.NCover.Reg" value="selected" />
          <param name="dotNetCoverage.NCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.Reg" value="selected" />
          <param name="dotNetCoverage.NCover3.args" value="//ias .*" />
          <param name="dotNetCoverage.NCover3.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover3.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.reporter.executable.args" value="//or FullCoverageReport:Html:{teamcity.report.path}" />
          <param name="dotNetCoverage.PartCover.Reg" value="selected" />
          <param name="dotNetCoverage.PartCover.includes" value="[*]*" />
          <param name="dotNetCoverage.PartCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.PartCover.platformVersion" value="v2.0" />
          <param name="dotNetTestRunner.Type" value="NUnit" />
          <param name="nunit_categery_exclude" value="Unreliable" />
          <param name="nunit_enabled" value="checked" />
          <param name="nunit_environment" value="v4.0" />
          <param name="nunit_include"><![CDATA[output/%waf.variant%/bin/Peach.Core.Test.dll
output/%waf.variant%/bin/Peach.Pro.Test.dll
output/%waf.variant%/bin/%PlatformTest%
output/%waf.variant%/bin/Godel.Tests.dll]]></param>
          <param name="nunit_platform" value="MSIL" />
          <param name="nunit_process_per_process" value="true" />
          <param name="nunit_version" value="NUnit-2.6.3" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="PeachPro_Dejavu" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="remoteRunOnBranch" type="remoteRunOnBranch">
        <parameters>
          <param name="branchy:jetbrains.git" value="pattern:jetbrains.git" />
          <param name="pattern:jetbrains.git" value="refs/heads/personal/*" />
        </parameters>
      </build-trigger>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:master" />
          <param name="quietPeriodMode" value="USE_DEFAULT" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <dependencies>
      <depend-on sourceBuildTypeId="PeachPro_Root">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</template>

