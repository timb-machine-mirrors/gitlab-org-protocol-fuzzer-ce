<?xml version="1.0" encoding="UTF-8"?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="b7569c89-c891-4505-844b-ae588fe9d382" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>common_nightly</name>
  <settings>
    <options>
      <option name="buildNumberPattern" value="%build.vcs.number%" />
      <option name="checkoutMode" value="ON_AGENT" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="configure" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="waf%waf.ext%" />
          <param name="command.parameters" value="configure --strict" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="build" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="waf%waf.ext%" />
          <param name="command.parameters" value="build --strict --variant=%waf.variant%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_9" name="install" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="waf%waf.ext%" />
          <param name="command.parameters" value="install --strict --variant=%waf.variant%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_10" name="" type="NUnit">
        <parameters>
          <param name="dotNetCoverage.NCover.HTMLReport.File.Sort" value="0" />
          <param name="dotNetCoverage.NCover.HTMLReport.File.Type" value="1" />
          <param name="dotNetCoverage.NCover.Reg" value="selected" />
          <param name="dotNetCoverage.NCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.Reg" value="selected" />
          <param name="dotNetCoverage.NCover3.args" value="//ias .*" />
          <param name="dotNetCoverage.NCover3.platformBitness" value="x86" />
          <param name="dotNetCoverage.NCover3.platformVersion" value="v2.0" />
          <param name="dotNetCoverage.NCover3.reporter.executable.args" value="//or FullCoverageReport:Html:{teamcity.report.path}" />
          <param name="dotNetCoverage.PartCover.Reg" value="selected" />
          <param name="dotNetCoverage.PartCover.includes" value="[*]*" />
          <param name="dotNetCoverage.PartCover.platformBitness" value="x86" />
          <param name="dotNetCoverage.PartCover.platformVersion" value="v2.0" />
          <param name="dotNetTestRunner.Type" value="NUnit" />
          <param name="nunit_enabled" value="checked" />
          <param name="nunit_environment" value="v4.0" />
          <param name="nunit_include"><![CDATA[output/%waf.variant%/bin/Peach.Core.Test.dll
output/%waf.variant%/bin/Peach.Pro.Test.dll
output/%waf.variant%/bin/%PlatformTest%
output/%waf.variant%/bin/Godel.Tests.dll
output/%waf.variant%/bin/PitTester.exe]]></param>
          <param name="nunit_platform" value="MSIL" />
          <param name="nunit_process_per_process" value="true" />
          <param name="nunit_version" value="NUnit-2.6.3" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="PeachPro_Dejavu" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</template>

