<Window x:Class="PeachFuzzFactory.UnhandledException"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UnhandledException" Height="300" Width="300" Closing="Window_Closing">
  <Grid>
    <TextBox HorizontalAlignment="Stretch" VerticalAlignment="Stretch" x:Name="txtMessage" AcceptsReturn="True" AcceptsTab="True" IsReadOnly="True" TextWrapping="Wrap" />
  </Grid>
</Window>
