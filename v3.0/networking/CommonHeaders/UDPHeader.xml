<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://peachfuzzer.com/2012/Peach ../peach.xsd" 
       author="Deja Vu Security, LLC" description="User Datagram Protocol PIT" version="0.0.2">
  <!-- Copyright (c) Deja Vu Security, LLC 2012 -->
		
	<!--###Override Checksum field with:
			<Number name="Checksum" size="16" endian="big">
        <Fixup class="UDPChecksumFixup">
          <Param name="ref" value="TCP" />  This should be the entire UDPHeader + Data
          <Param name="src" value="##SourceIPv4(6)##" />
          <Param name="dst" value="##TargetIPv4(6)##" />
        </Fixup>
      </Number>

	###-->
	<DataModel name="UDPHeader">
    <Number name="SrcPort" size="16" endian="network"/>
    <Number name="DestPort" size="16" endian="network"/>
    <Number name="Length" size="16" endian="network">
			<!--###Should be set to the entire packet (UDPHeader + Payload)###-->
      <Relation type="size" of="UDPHeader" />
    </Number>
    <Number name="Checksum" size="16" endian="big"/>
  </DataModel>
</Peach>
<!-- end -->
