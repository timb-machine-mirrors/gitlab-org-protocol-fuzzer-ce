		<Block name="Options"/>
	</DataModel>

	<!-- #################### Packet Definitions #################### -->
	<DataModel name="IPv6EmptyPacket">
		<Block ref="IPv6Ethernet"/>
		<Block ref="IPv6Header">
			<Number name="Header.NextHeader" value="59"/>
			<Number name="Header.PayloadLength" value="0" size="16"/>
		</Block>
	</DataModel>

	<DataModel name="IPv6ICMPPacket">
		<Block ref="IPv6Ethernet"/>
		<Block ref="IPv6Header">
			<Number name="Header.PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload" />
			</Number>
			<Number name="Header.NextHeader" value="58"/>
		</Block>
		<Blob name="Payload" valueType="hex" value="800014350a440001617bef50000000004e6a030000000000101112131415161718191a1b1c1d1e1f202122232425262728292a2b2c2d2e2f3031323334353637"/>
	</DataModel>

	<DataModel name="IPv6UDPTestPacket">
		<Block ref="IPv6Ethernet"/>
		<Block ref="IPv6Header">
			<Number name="Header.PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload" />
			</Number>
			<Number name="Header.NextHeader" value="17"/>
		</Block>
		<Block name="Payload" ref="UDPv6Header">
			<Number name="SrcPort" size="16" endian="network" value="53526"/>
			<Number name="DestPort" size="16" endian="network" value="9999"/>
			<Number name="Length" size="16" endian="network">
				<Relation type="size" of="Payload" />
			</Number>
			<Number name="Checksum" size="16" endian="network">
