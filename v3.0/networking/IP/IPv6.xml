	<DataModel name="IPv6HopByHopPacket">
		<Block ref="IPv6Ethernet"/>
		<Block ref="IPv6Header">
			<Number name="Header.PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload" />
			</Number>
			<Number name="Header.NextHeader" value="0"/>
		</Block>
		<Block name="Payload">
			<Block ref="HopByHop"/>
		</Block>
	</DataModel>

	<DataModel name="IPv6DestinationPacket">
		<!-- Destination options are structured in the same way as Hop-By-Hop options -->
		<Block ref="IPv6Ethernet"/>
		<Block ref="IPv6Header">
			<Number name="Header.PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload" />
			</Number>
			<Number name="Header.NextHeader" value="60"/>
		</Block>
		<Block name="Payload">
			<Block ref="Destination"/>
		</Block>
	</DataModel>

	<DataModel name="IPv6JumbogramPacket">
		<Block ref="IPv6Ethernet"/>
		<Block ref="IPv6Header">
			<Number name="Header.PayloadLength" size="16" endian="big" value="0000" valueType="hex">
			<!-- <Number name="Header.PayloadLength" size="16" endian="big" value="FFFF" valueType="hex"> -->
				<!-- <Relation type="size" of="Payload"/> -->
			</Number>
			<!-- wireshark doesn't parse this ^ if 0, RFC says should be 0 for payloads over 65535 -->
			<Number name="Header.NextHeader" value="0"/>
		</Block>
		<Block name="Payload">
			<Block ref="JumboFrame"/>
		</Block>
	</DataModel>

