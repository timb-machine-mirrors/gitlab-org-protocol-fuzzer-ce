					<Relation type="size" of="Payload" expressionGet="size" expressionSet="(((size-8) > 0) and ((size-8)/8)) or 2"/>
				</Number>
				<Number name="MH" size="8" endian="big" value="7"/>
				<Number name="Checksum" size="16" endian="big">
