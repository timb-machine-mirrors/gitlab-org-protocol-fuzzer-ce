				</Block>
			</Block>
		</Block>
	</DataModel>

	<!--Mobile IPv6 adds the "H" flag to Router Advertisement-->
	<!--Mobile IPv6  extends the Prefix Information Option-->
	<!--Mobile IPv6 adds the Advertisement Interval Option-->
	<!-- See http://tools.ietf.org/html/rfc6275#section-7.1 -->
	<DataModel name="IPv6MobileICMPRouterAdvertismentPacket">
		<Block ref="IPv6Ethernet"/>
		<Block ref="IPv6Header">
			<Number name="Header.PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload"/>
			</Number>
			<Number name="Header.NextHeader" value="58"/>
		</Block>
		<Block name="Payload">
			<Block ref="MobileICMPRouterAdvertisement">
				<Number name="Type" size="8" endian="big" value="134"/>
				<Number name="Code" size="8" endian="big" value="0"/>
				<Number name="Checksum" size="16" endian="big">
					<Fixup class="IcmpV6ChecksumFixup">
						<Param name="ref" value="Payload"/>
						<Param name="src" value="##SourceIPv6##"/>
						<Param name="dst" value="##TargetIPv6##"/>
					</Fixup>
				</Number>
				<Flags name="Flags" size="8">
					<Flag name="M" size="1" position="0"/>
					<Flag name="O" size="1" position="1"/>
					<Flag name="H" size="1" position="2"/>
					<Flag name="Reserved" size="5" position="3"/>
				</Flags>
				<Block name="Options">
					<Block name="PrefixInformation">
						<Number name="Type" size="8" endian="big" value="3"/>
						<Number name="Length" size="8" endian="big" value="4"/>
						<Number name="PrefixLength" size="8" endian="big"/>
						<Flags size="8" endian="big">
							<Flag name="L" size="1" position="0"/>
							<Flag name="A" size="1" position="1"/>
							<Flag name="R" size="1" position="2"/>
							<Flag name="Reserved" size="5" position="3"/>
						</Flags>
						<Number name="ValidLifeTime" size="32" endian="big" signed="false"/>
						<Number name="PreferredLifetime" size="32" endian="big" signed="false"/>
						<Number name="Reserved2" size="32" endian="big"/>
						<Blob name="Prefix" length="16" valueType="hex"/>
					</Block>
					<Block name="AdvertisementInterval">
						<Number name="Type" size="8" endian="big" value="7"/>
						<Number name="Length" size="8" endian="big" value="1"/>
						<Number name="Reserved" size="16" endian="big"/>
						<Number name="AdvertisementInterval" size="32" endian="big"/>
					</Block>
				</Block>
			</Block>
		</Block>
	</DataModel>

	<!-- #################### Agents #################### -->
	<Agent name="RemoteAgent" location="tcp://##RemoteAgentIP##:9001">
		<Monitor class="Process">
			<Param name="Executable" value="##tsharkBin##" />
			<Param name="Arguments" value="-i ##tsharkInterface##" />
			<Param name="FaultOnEarlyExit" value="true" />
		</Monitor>
		<Monitor class="Pcap">
			<Param name="Device" value="##Interface##" />
		</Monitor>
	</Agent>

	<!-- #################### State Models #################### -->
	<StateModel name="TheState" initialState="Initial">
		<State name="Initial">
			<Action type="output">
				<DataModel ref="IPv6ICMPPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6EmptyPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6JumbogramPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6HopByHopPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6DestinationPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6FragmentPacket1"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6FragmentPacket2"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6FragmentPacket3"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileBRRPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileHoTIPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileCoTIPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileHoTPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileCoTPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileBUPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileBAPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileBEPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileRoutingType2HeaderPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileICMPHomeAgentAddressDiscoveryRequestPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileICMPHomeAgentAddressDiscoveryReplyPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileICMPPrefixSolicitationPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileICMPPrefixAdvertisementPacket"/>
			</Action>
			<Action type="output">
				<DataModel ref="IPv6MobileICMPRouterAdvertismentPacket"/>
			</Action>
		</State>
	</StateModel>

	<!-- ####################### Tests ######################## -->
	<Test name="Default">
		<!-- <Agent ref="RemoteAgent"/> -->
		<StateModel ref="TheState"/>
		<Publisher class="RawEther">
			<Param name="Interface" value="##Interface##"/>
			<Param name="Protocol" value="ETH_P_IPV6"/>
		</Publisher>
		<Strategy class="##Strategy##"/>
		<Logger class="logger.Filesystem" >
			<Param name="Path" value="##LoggerPath##" />
		</Logger>
	</Test>
</Peach>
