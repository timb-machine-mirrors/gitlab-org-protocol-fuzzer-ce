						<Param name="dst" value="##TargetIPv6##"/>
					</Fixup>
				</Number>
			</Block>
			<Number name="Status" size="8" signed="false"/>
			<Number name="Reserved" size="8"/>
			<Blob name="HomeAddress" length="16"/>
