<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			 xsi:schemaLocation="http://peachfuzzer.com/2012/Peach /peach/peach.xsd"  
       author="Deja Vu Security, LLC" description="Ico PIT States" version="0.0.2">
	<!-- Copyright (c) Deja Vu Security, LLC 2013 -->

	<Include ns="DataModels" src="file:##Path##/_Common/Models/Image/Ico_State.xml"/>

	<Agent name="LocalAgent">
		<Monitor class="WindowsDebugger">
			<!-- The target application and fuzzed ico -->
			<Param name="CommandLine" value="mspaint.exe fuzzed.ico"/>
			<Param name="StartOnCall" value="LaunchPaint" />
		</Monitor>

		<Monitor class="PageHeap">
			<Param name="Executable" value="mspaint.exe"/>
		</Monitor>
	</Agent>

	<Test name="Default">
		<Agent ref="LocalAgent"/>
		<StateModel ref="State"/>

		<Publisher class="File">
			<Param name="FileName" value="fuzzed.ico"/>
		</Publisher>
		
		<Strategy class="##Strategy##"/>

		<Logger class="File">
			<Param name="Path" value="##LoggerPath##"/>
		</Logger>
	</Test>
</Peach>
<!-- end -->
