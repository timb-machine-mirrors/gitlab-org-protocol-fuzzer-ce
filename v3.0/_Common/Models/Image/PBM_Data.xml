  <!-- ********************************** -->
	<DataModel name="PBM">
		<Block name="PBMHeader">
			<String value="P1\n" token="true" />
			<Block name="Desc" minOccurs="0">
				<String value="#" token="true" />
				<String value= "Description" />
				<String value= "\n" token="true" />
			</Block>
		</Block>
		<Block name="PBMdata" >
			<String value="250">
				<Hint name="NumericalString" value="true" />
			</String>
			<String value=" " token="true" />
			<String value="250">
				<Hint name="NumericalString" value="true" />
			</String>
			<String value= "\n" token="true" />
		</Block>
		<Block name="Data" minOccurs="1" >
			<String constraint="int(len(str(element.DefaultValue))) &lt; 71" />
			<String value= "\n" token="true" />
		</Block>
		<Blob />
		
	</DataModel>
</Peach>
