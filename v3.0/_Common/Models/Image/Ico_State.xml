<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			 xsi:schemaLocation="http://peachfuzzer.com/2012/Peach /peach/peach.xsd"  
       author="Deja Vu Security, LLC" description="Ico PIT States" version="0.0.2">
	<!-- Copyright (c) Deja Vu Security, LLC 2013 -->
	
	<Include ns="DataModels" src="file:##Path##/_Common/Models/Image/Ico_Data.xml"/>
	
	<StateModel name="State" initialState="Initial">
		<State name="Initial">
			
			<!-- Write out our ico file -->
			<Action type="output">
				<DataModel ref="DataModels:ICO" />
				<!-- This is our sample file to read in -->
				<Data fileName="##Path##/_Common/Samples/Image/##Seed##" />
			</Action>
		
			<Action type="close" />
			
			<!-- Launch the target process -->
			<Action type="call" method="LaunchPaint" publisher="Peach.Agent"/>
		</State>
	</StateModel>
</Peach>
<!--END-->