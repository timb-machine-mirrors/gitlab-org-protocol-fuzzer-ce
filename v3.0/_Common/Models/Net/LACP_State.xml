<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://peachfuzzer.com/2012/Peach /peach/peach.xsd"  
       author="Deja Vu Security, LLC" description="Link Aggregation Control Protocol PIT StateModels" version="0.0.2">
  <!-- Copyright (c) Deja Vu Security, LLC 2013 -->

  <Include ns="Ethernet" src="file:##Path##/_Common/Models/Net/Ethernet_Data.xml"/>	
  <Include ns="LACP" src="file:##Path##/_Common/Models/Net/LACP_Data.xml"/>

  <DataModel name="LACPPacket" ref="Ethernet:Frame">
    <Blob name="Dest" valueType="hex" value="ffffffffffff"/>
    <Number name="TypeOrLen" token="true" valueType="hex" size="16" value="8809"/>
    <Block name="Payload" ref="LACP:Packet" />
  </DataModel>

  <!-- StateModel for All LACP requests  -->
  <StateModel name="Transmit" initialState="Initial">
    <State name="Initial">
      <Action type="output">
        <DataModel ref="LACPPacket"/>
      </Action>
    </State>
  </StateModel>
</Peach>
<!--End-->
