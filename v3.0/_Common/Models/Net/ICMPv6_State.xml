<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://peachfuzzer.com/2012/Peach ../peach.xsd"  
       author="Deja Vu Security, LLC" description="Internet Control Message Protocol Version 6 PIT StateModels" version="0.0.2">
  <!-- Copyright (c) Deja Vu Security, LLC 2013 -->
	
  <Include ns="ICMPv6" src="file:##Path##/_Common/Models/Net/ICMPv6_Data.xml"/>
  <Include ns="IPv6" src="file:##Path##/_Common/Models/Net/IPv6_Data.xml"/>
    
  <StateModel name="Transmit" initialState="Initial">
    <State name="Initial">
      
      <Action name="EchoRequest" type="output">
        <DataModel ref="ICMPv6:EchoRequest"/>
      </Action>
      
      <Action name="EchoReply" type="output">
        <DataModel ref="ICMPv6:EchoReply"/>
      </Action>
      
      <Action name="DestinationUnreachable" type="output"><!--Not Displaying on Wireshark?-->
        <DataModel ref="ICMPv6:DestinationUnreachable" />
      </Action>
      
      <Action name="PacketTooBig" type="output">
        <DataModel ref="ICMPv6:PacketTooBig" />
      </Action>
      
      <Action name="TimeExceeded" type="output">
        <DataModel ref="ICMPv6:TimeExceeded" />
      </Action>
      
      <Action name="ParameterProblem" type="output"><!--Not Displaying on Wireshark?-->
        <DataModel ref="ICMPv6:ParameterProblem" />
      </Action>

    </State>
  </StateModel>
</Peach>
<!--END-->
