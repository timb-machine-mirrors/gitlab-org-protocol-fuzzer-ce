<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://peachfuzzer.com/2012/Peach ../peach.xsd"  
       author="Deja Vu Security, LLC" description="Internet Group Message Protocol PIT DataModels" version="0.0.2">
			 
	<!-- Copyright (c) Deja Vu Security, LLC 2013 -->
			 
	<DataModel name="IGMPHeader">
    <Number name="Type" size="8" value="0x11"/>
    <Number name="Code" size="8" endian="big"/>
    <Number name="Checksum" size="16" endian="big">
      <Fixup class="IcmpChecksumFixup">
				<Param name="ref" value="IGMPHeader"/>
      </Fixup>
    </Number>
    <Blob name="GroupAddress" length="4"/>
    <Flags size="8" endian="big">
      <Flag name="Resv" size="4" position="0"/>
      <Flag name="Suppress" size="1" position="4"/>
      <Flag name="QRV" size="3" position="5"/>
    </Flags>
    <Number name="QQIC" size="8" endian="big"/>
    <Number name="NumSources" size="16" endian="big"/>
    <Block name="SourceAddresses">
      <Blob/>
    </Block>
  </DataModel>
</Peach>
<!-- end -->