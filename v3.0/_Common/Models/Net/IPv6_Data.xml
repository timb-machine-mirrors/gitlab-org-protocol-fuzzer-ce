	<DataModel name="IPv6MobileBAPacket">
		<Block ref="Ethernet:Frame"/>
		<Block ref="IPv6Header">
			<Number name="PayloadLength" size="16" endian="big">
				<Relation type="size" of="Payload"/>
			</Number>
			<Number name="NextHeader" size="8" value="135"/>
		</Block>
		<Block name="Payload">
			<Block ref="Mobile">
				<Number name="HeaderLen" size="8" endian="big">
