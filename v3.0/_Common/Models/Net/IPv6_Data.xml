	<Include ns="Ethernet" src="file:##Path##/_Common/Models/Net/Ethernet_Data.xml"/>
	<Include ns="UDPv6" src="file:##Path##/_Common/Models/Net/UDPv6_Data.xml"/>
	<Include ns="ICMP" src="file:##Path##/_Common/Models/Net/ICMPv6_Data.xml"/>
	
	<!--###Be sure to override the NextHeader Value##-->
	<DataModel name="TLV">
		<Number name="Type" size="8" endian="big"/>
		<Number name="Length" size="8">
			<Relation type="size" of="Value"/>
		</Number>
		<Block name="Value"/>
	</DataModel>

	<DataModel name="IPv6Header">
		<Flags size="32" endian="big">
			<Flag name="Version" position="0" size="4" valueType="hex" value="6"/>
			<Flag name="DSCP" position="4" size="6" valueType="hex" value="00"/>
			<Flag name="ECN" position="10" size="2" valueType="hex" value="00"/>
			<Flag name="FlowLabel" position="12" size="20" valueType="hex" value="000000"/>
		</Flags>
		<Number name="PayloadLength" size="16" endian="big" valueType="hex" value="ffff"/>
		<Number name="NextHeader" size="8" endian="big" value="59"/>
		<Number name="HopLimit" size="8" endian="big" valueType="hex" value="40"/>
		<Block name="SrcBlock" length="16">
			<Blob name="SrcIP" value="##SourceIPv6##">
				<Transformer class="Ipv6StringToOctet"/>
			</Blob>
		</Block>
		<Block name="DstBlock" length="16">
			<Blob name="DestIP" value="##TargetIPv6##">
				<Transformer class="Ipv6StringToOctet"/>
			</Blob>
		</Block>
		<Block name="IPv6Payload"/>
	</DataModel>
	
	<DataModel name="HopByHop">
		<Block name="HopByHopContents">
			<Number name="NextHeader" size="8" value="59"/>
			<Number name="PayloadLength" value="0" size="8">
				<Relation type="size" of="HopByHopContents" expressionSet="((size-8)/8) + int(bool(size % 8))" expressionGet="((size+1)*8)"/>
			</Number>
			<Block name="TLVs">
				<!-- size relation with padding -->
				<Block name="InternalTLV" ref="TLV" minOccurs="0" maxOccurs="255">
					<Number name="Type" size="8" value="0"/>
				</Block>
				<Block name="Padding" minOccurs="1">
					<Number name="Type" size="8" value="1"/>
					<Number name="Length" size="8" value="0">
						<Relation type="size" of="PaddingValue"/>
					</Number>
				</Block>
			</Block>
			<Padding name="PaddingValue"  alignment="64" alignedTo="HopByHopContents"/>
		</Block>
	</DataModel>
	
	<DataModel name="Destination">
		<Block ref="HopByHop">
			<Block name="HopByHopContents.TLVs.InternalTLV">
				<Block ref="TLV" name="HomeAddressOption">
					<Number name="Type" size="8" endian="big" value="201"/>
					<Block name="Value">
						<Blob length="16" valueType="hex"/>
					</Block>
				</Block>
			</Block>
		</Block>
	</DataModel>

	<DataModel name="JumboFrame">
		<Block name="JumboHopByHop">
			<Block name="JumboHopByHopContents">
				<Number name="NextHeader" size="8" value="17"/>
				<Number name="PayloadLength" value="255" size="8">
					<Relation type="size" of="JumboHopByHopContents"
										expressionSet="((size-8)/8) + int(bool(size % 8))"
										expressionGet="((size+1)*8)" /><!-- this is length in 8-octet units -->
				</Number>
				<Block name="InternalTLV" ref="TLV" minOccurs="1" maxOccurs="255">
					<Number name="Type" size="8" value="c2" valueType="hex"/>
					<Number name="Length" size="8" value="0">
						<Relation type="size" of="Value"/>
					</Number>
					<Block name="Value">
						<Number name="SizeOfFrame" size="32">
							<Relation type="size" of="PacketPayload"/>
						</Number>
					</Block>
				</Block>
			</Block>
			<Padding name="PaddingValue"  alignment="64" alignedTo="JumboHopByHop"/>
		</Block>
		<!-- We're doing a UDP payload so this can be tested  -->
		<Block name="PacketPayload">
			<Number name="SrcPort" size="16" endian="network" value="52161"/>
			<Number name="DestPort" size="16" endian="network" value="9999"/>
			<Number name="Length" size="16" endian="network" value="65527">
				<Relation type="size" of="PacketPayload" />
			</Number>
			<Number name="Checksum" size="16" endian="network">
				<Fixup class="UDPChecksumFixup">
					<Param name="ref" value="PacketPayload" />
					<Param name="src" value="##SourceIPv4##" />
					<Param name="dst" value="##TargetIPv4##" />
				</Fixup>
			</Number>
			<Block name="Data" >
				<Block minOccurs="511" maxOccurs="16777216">
					<Blob value="AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA"/>
				</Block>
				<Blob value="AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA"/>
			</Block>
		</Block>
	</DataModel>

	<DataModel name="Fragment">
		<Number name="NextHeader" size="8" value="59" endian="big"/>
		<Number name="Reserved" size="8" endian="big"/>
		<Flags name="Flags" size="16" endian="big">
			<Flag name="Offset" size="13" position="0"/>
			<Flag name="Res" size="2" position="13"/>
			<Flag name="Mflag" size="1" position="15"/>
		</Flags>
		<Number name="Identification" size="32" endian="big"/>
		<Blob name="Data" length="12"/>
	</DataModel>
	
	<DataModel name="Routing">
		<Number name="NextHeader" size="8" value="59" endian="big"/>
		<Number name="HdrExtLen" size="8" endian="big" signed="false">
			<Relation type="size" of="Data" expressionGet="size*8" expressionSet="size/8"/>
		</Number>
		<Number name="RoutingType" size="8" value="1"/>
		<Number name="SegmentsLeft" size="8"/>
		<Blob name="Data" valueType="hex"/>
	</DataModel>

	<DataModel name="DestinationOptions">
			<Number name="NextHeader" size="8" endian="big" value="59"/>
			<Number name="HdrExtLen" size="8" endian="big" signed="false">
				<Relation type="size" of="Options" expressionGet="size*8" expressionSet="size/8"/>
			</Number>
			<Block name="Options" ref="TLV" minOccurs="1" maxOccurs="-1"/>
	</DataModel>

	<DataModel name="Mobile">
		<Number name="NextHeader" size="8" value="59" endian="big"/>
		<Number name="HeaderLen" size="8" endian="big">
		</Number>
		<Number name="MH" size="8" endian="big"/>
		<Number name="Reserved" size="8" endian="big"/>
		<Number name="MHChecksum" size="16" endian="big">
		</Number>
		<Block name="MobilityOptions"/>
	</DataModel>

	<DataModel name="MobileRoutingType2Header">
		<Number name="NextHeader" size="8" endian="big"/>
		<Number name="HdrExtLen" size="8" endian="big" signed="false" value="2"/>
		<Number name="RoutingType" size="8" endian="big" signed="false" value="2"/>
		<Number name="SegmentsLeft" size="8" endian="big" signed="false" value="1"/>
		<Number name="Reserved" size="32" endian="big"/>
		<Blob name="HomeAddress" valueType="hex" length="16"/>
	</DataModel>
	
