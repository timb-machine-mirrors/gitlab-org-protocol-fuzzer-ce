<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://peachfuzzer.com/2012/Peach ../peach.xsd"  
       author="Deja Vu Security, LLC" description="Internet Control Message Protocol v4 PIT DataModels" version="0.0.2">
	<!-- Copyright (c) Deja Vu Security, LLC 2013 -->

  <DataModel name="Packet">
    <Block name="Content">
      <Number name="Type" size="8"/>
      <Number name="Code" size="8"/>
      <Number name="Checksum" endian="big" size="16">
        <Fixup class="IcmpChecksumFixup">
          <Param name="ref" value="Content" />
        </Fixup>
      </Number>
      <Blob name="Remainder" length="4"/>
      <Blob name="Payload" value="Hello Peach World"/>
    </Block>
  </DataModel> 
  
  <DataModel name="EchoRequest" ref="Packet">
    <Number name="Content.Type" size="8" value="8" token="true" />
  </DataModel>
  
  <DataModel name="EchoReply" ref="Packet">
    <Number name="Content.Type" size="8" value="0" token="true" />
  </DataModel>
  
  <DataModel name="DestinationUnreachable" ref="Packet">
    <Number name="Content.Type" size="8" value="3" token="true" />
  </DataModel>
  
  <DataModel name="SourceQuench" ref="Packet">
    <Number name="Content.Type" size="8" value="4" token="true" />
  </DataModel>
  
  <DataModel name="Redirect" ref="Packet">
    <Number name="Content.Type" size="8" value="5" token="true" />
  </DataModel>
  
  <DataModel name="TimeExceeded" ref="Packet">
    <Number name="Content.Type" size="8" value="11" token="true" />
  </DataModel>
  
  <DataModel name="ParameterProblem" ref="Packet">
    <Number name="Content.Type" size="8" value="12" token="true" />
  </DataModel>
  
  <DataModel name="Timestamp" ref="Packet">
    <Number name="Content.Type" size="8" value="13" token="true" />
  </DataModel>
  
  <DataModel name="TimestampReplyProblem" ref="Packet">
    <Number name="Content.Type" size="8" value="14" token="true" />
  </DataModel>
  
  <DataModel name="InformationRequest" ref="Packet">
    <Number name="Content.Type" size="8" value="15" token="true" />
  </DataModel>
  
  <DataModel name="InformationReply" ref="Packet">
    <Number name="Content.Type" size="8" value="16" token="true" />
  </DataModel>
  
  <DataModel name="RouterAdvertisement" ref="Packet">
    <Number name="Content.Type" size="8" value="9" token="true" />
  </DataModel>
  
  <DataModel name="RouterSolicitation" ref="Packet">
    <Number name="Content.Type" size="8" value="10" token="true" />
  </DataModel>

</Peach>
<!--END-->
