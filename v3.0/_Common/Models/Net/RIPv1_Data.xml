<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://peachfuzzer.com/2012/Peach ../peach.xsd"
        author="Deja Vu Security, LLC" description="Routing Information Protocol v1 DataModels" version="0.0.2">
  <!-- Copyright (c) Deja Vu Security, LLC 2013 -->
  <!--RFC1058: Implements all features-->
  <DataModel name="Packet">
    <Block name="Header">
		<Number name="Command" size="8" value="2" />
		<Number name="Version" size="8" value="1" />
		<Number name="Mustbezero" size="16" token="true" /> 

    </Block>
    <Block name="RIPv1Payload" minOccurs="1" maxOccurs="25" >
		<Number name="FamilyID" size="16" value="00 02" valueType="hex"/>
		<Number name="Mustbezero1" size="16" token="true" />
		
		<Number name="IP" size="32" value="C0120103"  valueType="hex" />
		<Number name="Mustbezero2" size="32" token="true" />
		<Number name="Mustbezero3" size="32"  token="true" />
		<Number name="Metricfill" size="24" token="true" /> 
		<Number name="Metric" size="8" value="5" /> 

    </Block>	
	
  </DataModel>
</Peach>
<!-- end -->	