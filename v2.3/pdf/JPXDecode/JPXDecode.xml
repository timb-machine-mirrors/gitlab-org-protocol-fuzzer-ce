	<DataModel name="BoxCompositingLayerHeader" ref="Box">
		<Blob name="Type" length="4" value="6A 70 6C 68" valueType="hex" isStatic="true"/>
		<Block name="Data">
			<Relation type="size" from="LengthNormal"/>
			<Relation type="size" from="LengthExtended"/>
