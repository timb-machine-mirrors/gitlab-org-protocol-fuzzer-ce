			<Block name="TheBoxUUID" ref="BoxUUID"/>
			<Block name="TheBoxUUIDList" ref="BoxUUIDList"/>
			<Block name="TheBoxURL" ref="BoxURL"/>
			<Block name="TheBoxUUIDInfo" ref="BoxUUIDInfo"/>
