                <Number name="DN" size="16" endian="big" value="17" constraint="int(value) == 17"/>
                <Number name="ASN" size="16" endian="big" value="18" constraint="int(value) == 18"/>
                <Number name="XTP_IPv4" size="16" endian="big" value="19" constraint="int(value) == 19"/>
