{
	"qa": [
		{
			"id": 0,
			"type": "intro",
			"qref":"/partials/data-intro.html",
			"next":1
		},
		{
			"id": 1,
			"q": "Do you want to perform data collection on your local machine or a remote machine?",
			"type": "choice",
			"key":"IsLocal",
			"choice": [
				{ "value":true,	"a": "From a local machine.",	"next":4 },
				{ "value":false, "a": "From remote machine.","next":2 },
				{ "a": "Do not perform data collection." }
			]
		},
		{
			"id":2,
			"q": "What is the remote machine hostname or IP address?",
			"type": "string",
			"key": "AgentUrl",
			"next": 3
		},
		{
			"id":3,
			"q":"What is the remote machine operating system?",
			"type":"choice",
			"key":"OS",
			"choice":[
				{ "value":"windows", "a":"Windows", "next": 4000 },
				{ "value":"osx", "a":"Apple OS X", "next": 4000 },
				{ "value":"linux", "a":"Linux (Ubuntu, Redhat, SUSE, etc.)", "next": 4000 }
			],
			"next":4
		},
		{
			"id":4,
			"q":null,
			"type":"jump",
			"key":"LocalOS",
			"choice":[
				{ "value":"windows", "next":4000 },
				{ "value":"osx", "next":4000 },
				{ "value":"linux", "next":4000 }
				]
		},
		{
			"id": 4000,
			"q": "What type of data collection would you like to perform?",
			"type": "choice",
			"choice": [
				{ "value": 0, "a": "Take a network capture of traffic that occurs during the fuzzing iteration.", "next":4100 },
				{ "value": 1, "a": "Save a file generated locally to the machine performing data collection.","next":4200 },
				{ "value": 2, "a": "Save a remote file via SSH.","next":4300 },
				{ "value": 3, "a": "Save log messages collected via the serial port.", "next":4400 },
				{ "value": 4, "a": "Save log messaages collected via syslog.", "next":4500 }
			]
		},
		{
			"id":4100,
			"q": "Which network device should be captured? For Windows, the network interfaces can be shown by running 'ipconfig'. On Linux and OS X, the network interfaces can be shown by running the command 'ifconfig'. Example interface names are 'Local Area Connection' and Ethernet on Windows, 'eth0' on Linux, and 'en0' on OS X.",
			"type":"iface",
			"key":"PcapDevice",
			"next":4101
		},
		{
			"id":4101,
			"q":"Provide a PCAP filter rule to limit the network traffic collected (optional). If no filter is provided, all traffic will be collected. A guide to writing PCAP filter rules is available at http://google.com/?q=pcap+filter.",
			"type":"string",
			"required": false,
			"key":"PcapFilter"
		},
		{
			"id":4200,
			"q":"Enter the file name you would like to save. The file will be collected at the end of a faulting iteration. The file will not be deleted.",
			"type":"string",
			"key":"SaveFileFilename"
		},
		{
			"id":4300,
			"q":" Enter the SSH remote host hostname or IP address.",
			"type":"string",
			"key":"SshDownloaderHost",
			"next":4301
		},
		{
			"id":4301,
			"q":"What username would you like to use? If this field is left blank, Peach will use the current users username.",
			"type":"string",
			"required":false,
			"key":"SshDownloaderUsername",
			"next":4302
		},
		{
			"id":4302,
			"q":"What is the authentication type?",
			"type":"choice",
			"choice":[
				{"value": 0, "a":"Password authentication.", "next": 4310},
				{"value": 1, "a":"Private key.", "next": 4320}
			]
		},
		{
			"id":4310,
			"q":"Enter the remote host authentication password.",
			"type":"string",
			"required": false,
			"key":"SshDownloaderPassword",
			"next":4330
		},
		{
			"id":4320,
			"q":"Enter the full private key file pathname that Peach will use to authenticate the remote host.",
			"type":"string",
			"required": false,
			"key":"SshDownloaderKeyPath",
			"next":4330
		},
		{
			"id":4330,
			"q":"Would you like to download a single file or a folder of files?",
			"type":"choice",
			"choice":[
				{"value": 0, "a":"Download a single file.", "next":4340},
				{"value":	1, "a":"Download a folder of files.", "next":4350}
			]
		},
		{
			"id":4340,
			"q":"Enter the filename (full pathname) to download.",
			"type":"string",
			"required": false,
			"key":"SshDownloaderFile",
			"next":4360
		},
		{
			"id":4350,
			"q":"Enter the download folder name.",
			"type":"string",
			"required": false,
			"key":"SshDownloaderFolder"
		},
		{
			"id":4360,
			"q":"Should Peach remove the file after it's downloaded?",
			"type":"choice",
			"key":"SshDownloaderRemove",
			"choice":[
				{"value":true, "a":"Yes, remove the file after downloading it." },
				{"value": false, "a":"No, do not remove the file."}
				]
		},
		{
			"id":4400,
			"q":"What is the serial port the target is connected to?",
			"type":"string",
			"key":"SerialPortPort",
			"next":4401,
			"required":true
		},
		{
			"id":4401,
			"q":"What baud rate should be used to communicate with the target?",
			"key":"SerialPortBaudRate",
			"type":"range",
			"rangeMin":1,
			"rangeMax":999999,
			"value":115200,
			"next":4402,
			"required": true
		},
		{
			"id":4402,
			"q":"What is the number of data bits per byte?",
			"key":"SerialPortDataBits",
			"type":"choice",
			"choice":[
				{ "value":"8","a":"8","next":4403 },
				{ "value":"7","a":"7","next":4403 },
				{ "value":"6","a":"6","next":4403 },
				{ "value":"5","a":"5","next":4403 }
			],
			"value":"8",
			"required": true
		},
		{
			"id":4403,
			"q":"What is the parity-checking protocol to use?",
			"key":"SerialPortParity",
			"type":"choice",
			"choice":[
				{ "value":"None","a":"None","next":4404 },
				{ "value":"Odd","a":"Odd","next":4404 },
				{ "value":"Even","a":"Even","next":4404 },
				{ "value":"Mark","a":"Mark","next":4404 },
				{ "value":"Space","a":"Space","next":4404 }
			],
			"value":"None",
			"required": true
		},
		{
			"id":4404,
			"q":"What is the number of stop bits per byte?",
			"key":"SerialPortStopBits",
			"type":"choice",
			"choice":[
				{ "value":"One","a":"1","next":4405 },
				{ "value":"OnePointFive","a":"1.5","next":4405 },
				{ "value":"Two","a":"2","next":4405 }
			],
			"value":"One",
			"required": true
		},
		{
			"id":4405,
			"q":"What flow control algoruthm should be used?",
			"key":"SerialPortHandshake",
			"type":"choice",
			"choice":[
				{ "value":"None","a":"None" },
				{ "value":"XOnXoff","a":"XON/XOFF" },
				{ "value":"RequestToSend","a":"RTS/CTS" },
				{ "value":"RequestToSendXOnXOff","a":"RTS/CTS & XON/XOFF" }
			],
			"value":"None",
			"required": true
		},
		{
			"id":4500,
			"q":"What IP address should be used to listen for syslog messages?",
			"type":"ipv4",
			"key":"SyslogInterface",
			"value":"0.0.0.0",
			"next":4501,
			"required":true
		},
		{
			"id":4501,
			"q":"What port should be used to listen for syslog messages?",
			"key":"SyslogPort",
			"type":"range",
			"rangeMin":1,
			"rangeMax":65535,
			"value":514,
			"required": true
		}
	],
	"monitors": [
		{
			"monitorClass":"Pcap",
			"path": [ 4100 ],
			"map":[
				{"key":"PcapDevice", "param":"Device"},
				{"key":"PcapFilter", "param":"Filter"}
				],
			"description":"Perform a network capture during the fuzzing iteration. "
		},
		{
			"monitorClass":"SaveFile",
			"path":[ 4200 ],
			"map":[
				{"key":"SaveFileFilename", "param":"Filename" }
				],
			"description":  "Save a file."
		},
		{
			"monitorClass":"SshDownloader",
			"path":[ 4300 ],
			"map":[
				{"key":"SshDownloaderHost", "param":"Host" },
				{"key":"SshDownloaderUsername", "param":"Username" },
				{"key":"SshDownloaderPassword", "param":"Password" },
				{"key":"SshDownloaderKeyPath", "param":"KeyPath" },
				{"key":"SshDownloaderFile", "param":"File" },
				{"key":"SshDownloaderFolder", "param":"Folder" },
				{"key":"SshDownloaderRemove", "param":"Remove" }
			],
			"description": "Save a specified file as part of the logged data when a fault occurs. "
		},
		{
			"monitorClass":"SerialPort",
			"path": [4400],
			"map": [
				{ "key":"SerialPortPort","param":"Port" },
				{ "key":"SerialPortBaudRate","param":"BaudRate" },
				{ "key":"SerialPortDataBits","param":"DataBits" },
				{ "key":"SerialPortParity","param":"Parity" },
				{ "key":"SerialPortStopBits","param":"StopBits" },
				{ "key":"SerialPortHandshake","param":"Handshake" }
			],
			"description": "Save log messages collected via the serial port. "
		},
		{
			"monitorClass":"Syslog",
			"path": [4500],
			"map": [
				{ "key":"SyslogInterface","param":"Interface" },
				{ "key":"SyslogPort","param":"Port" }
			],
			"description": "Save log messages collected via syslog. "
		}
	]
}
