{
	"state": [
		{"name":"OS", "value":"win"},
		{"name":"IpAddress", "value":"127.0.0.1"},
		{"name":"PingRetryCount", "value":"1"}
	],
	"qa": [
		{
			"id": 0,
			"type": "intro",
			"qref":"static/data-intro.html",
			"next":1
		},
		{
			"id": 1,
			"q": "Do you want to perform data collection on your local machine or a remote machine?",
			"type": "choice",
			"key":"IsLocal",
			"choice": [
				{ "value":true,	"a": "From local machine.",	"next":4 },
				{ "value":false, "a": "From remote machine.","next":2 }
			]
		},
		{
			"id":2,
			"q": "What is the hostname or IP address of the remote machine that will perform collection?",
			"type": "string",
			"key": "HostName",
			"next": 3
		},
		{
			"id":3,
			"q":"What is the operating system of the remote machine that will perform collection?",
			"type":"choice",
			"key":"OS",
			"choice":[
				{ "value":"win", "a":"Windows", "next": 4000 },
				{ "value":"osx", "a":"Apple OS X", "next": 4000 },
				{ "value":"lin", "a":"Linux (Ubuntu, Redhat, SUSE, etc.)", "next": 4000 }
			],
			"next":4
		},
		{
			"id":4,
			"q":null,
			"type":"jump",
			"key":"OS",
			"choice":[
				{ "value":"win", "next":4000 },
				{ "value":"osx", "next":4000 },
				{ "value":"lin", "next":4000 }
				]
		},
		{
			"id": 4000,
			"q": "What type of data collection would you like to perform?",
			"type": "choice",
			"choice": [
				{ "a": "Take a network capture of traffic that occurs during the fuzzing iteration.", "next":4100 },
				{ "a": "Save a file generated locally to the machine performing data collection.","next":4200 },
				{ "a": "Save a remote file via SSH.","next":4300 }
			]
		},
		{
			"id":4100,
			"q": "What is the network device to perform the network capture on?",
			"type":"interface",
			"key":"PcapDevice",
			"next":4101
		},
		{
			"id":4101,
			"q":"Optionally, provide a PCAP filter rule to limit the network traffic collected. If no filter is provided, all traffic will be collected. A guide to writing PCAP filter rules is available at http://google.com/?q=pcap+filter.",
			"type":"string",
			"required": false,
			"key":"PcapFilter"
		},
		{
			"id":4200,
			"q":"Enter the file name you would like to save. The file will be collected at the end of a faulting iteration. The file will not be deleted.",
			"type":"string",
			"key":"SaveFileFilename"
		},
		{
			"id":4300,
			"q":"What is the remote host that will be connected to using SSH? Enter the hostname or IP address.",
			"type":"string",
			"key":"SshDownloaderHost",
			"next":4301
		},
		{
			"id":4301,
			"q":"What username would you like to connect as? If this field is left blank, the current users username will be used.",
			"type":"string",
			"required":false,
			"key":"SshDownloaderUsername",
			"next":4302
		},
		{
			"id":4302,
			"q":"What type of authentication should be used?",
			"type":"choice",
			"choice":[
				{"q":"Password authentication.", "next": 4310},
				{"q":"Private key.", "next": 4320}
			]
		},
		{
			"id":4310,
			"q":"Enter the password to use for authenticating to the remote host.",
			"type":"string",
			"required": false,
			"key":"SshDownloaderPassword",
			"next":4330
		},
		{
			"id":4320,
			"q":"Enter full path to private key file to use for authentication to the remote host.",
			"type":"string",
			"required": false,
			"key":"SshDownloaderKeyPath",
			"next":4330
		},
		{
			"id":4330,
			"q":"Would you like to download a single file, or a folder of files?",
			"type":"choice",
			"choice":[
				{"a":"Download a single file.", "next":4340},
				{"a":"Download a folder of files.", "next":4350}
			]
		},
		{
			"id":4340,
			"q":"Enter the filename with full path to download.",
			"type":"string",
			"required": false,
			"key":"SshDownloaderFile",
			"next":4360
		},
		{
			"id":4350,
			"q":"Enter the folder to download.",
			"type":"string",
			"required": false,
			"key":"SshDownloaderFolder"
		},
		{
			"id":4360,
			"q":"Should the file be removed after being downloaded?",
			"type":"choice",
			"key":"SshDownloaderRemove",
			"choice":[
				{"value":true, "a":"Yes, remove the file after downloading it." },
				{"a":"No, do not remove the file.", "value": false}
				]
		}
	],
	"monitors": [
		{
			"monitorClass": "Agent",
			"path": [ 0 ],
			"map": [
				{"key":"AgentUrl", "param":"location" }
				]
		},
		{
			"monitorClass":"Pcap",
			"path": [ 4100 ],
			"map":[
				{"key":"PcapDevice", "param":"Device"},
				{"key":"PcapFilter", "param":"Filter"}
				]
		},
		{
			"monitorClass":"SaveFile",
			"path":[ 4200 ],
			"map":[
				{"key":"SaveFileFilename", "param":"Filename" }
				]
		},
		{
			"monitorClass":"SshDownloader",
			"path":[ 4300 ],
			"map":[
				{"key":"SshDownloaderHost", "param":"Host" },
				{"key":"SshDownloaderUsername", "param":"Username" },
				{"key":"SshDownloaderPassword", "param":"Password" },
				{"key":"SshDownloaderKeyPath", "param":"KeyPath" },
				{"key":"SshDownloaderFile", "param":"File" },
				{"key":"SshDownloaderFolder", "param":"Folder" },
				{"key":"SshDownloaderRemove", "param":"Remove" }
			]
		}
	]
}
