<Peach xmlns="http://peachfuzzer.com/2012/Peach"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://peachfuzzer.com/2012/Peach c:\peach\output\win_x64_debug\bin\peach.xsd"
       author="Deja Vu Security, LLC"
 description="Simple Network Management Protocol PIT DataModels" version="0.0.2">
	
	<Defaults>
		<Number endian="big"/>
	</Defaults>

	<!-- SNMP v2c Packet. Override PDU or VariableBindings.Value -->
	<DataModel name="SNMPHeaderV2">
		<Asn1Type name="SnmpAll" class="0" pc="1" tag="16">
			<Block name="Value">
				<Asn1Type name="Version" tag="2">
					<Number name="Value" size="8" value="1" token="true"/>
				</Asn1Type>

				<Asn1Type name="Community" tag="4">
					<String name="Value" value="##CommString##"/>
				</Asn1Type>

				<Asn1Type name="Request" pc="1" tag="16">
					<Block name="Value">
						<Asn1Type name="RequestId" tag="2">
							<Number name="Value" size="32" signed="false" value="0x72a020a6"/>
						</Asn1Type>
						<Asn1Type name="ErrorStatus" tag="2">
							<Number name="Value" size="8" signed="false" value="0"/>
						</Asn1Type>
						<Asn1Type name="ErrorIndex" tag="2">
							<Number name="Value" size="8" signed="false" value="0"/>
						</Asn1Type>

						<Asn1Type name="VariableBindingsList" pc="1" tag="16">
							<Asn1Type name="Value" pc="1" tag="16">
								<Blob name="Value" />
							</Asn1Type>
						</Asn1Type>
					</Block>
				</Asn1Type>
			</Block>
		</Asn1Type>
	</DataModel>

	<!-- Simple Network Management Protocol: GetRequest data model... -->
	<DataModel name="SnmpGet" ref="SNMPHeaderV2">
		<Asn1Type name="SnmpAll.Value.Request" class="2" pc="1" tag="0">
			<Block name="Value">
				<Asn1Type name="RequestId" tag="2">
					<Number name="Value" size="32" signed="false" value="0x72a020a6"/>
				</Asn1Type>
				<Asn1Type name="ErrorStatus" tag="2">
					<Number name="Value" size="8" signed="false" value="0"/>
				</Asn1Type>
				<Asn1Type name="ErrorIndex" tag="2">
					<Number name="Value" size="8" signed="false" value="0"/>
				</Asn1Type>

				<Asn1Type name="VariableBindings" pc="1" tag="16">
					<Asn1Type name="Value" pc="1" tag="16">
						<Block name="Value">
							<Asn1Type name="OID" tag="6">
								<!-- Object Name: 1.3.6.1.2.1.1.4.0 (iso.3.6.1.2.1.1.4.0) -->
								<Blob valueType="hex" value="2b 06 01 02 01 01 04 00"/>
							</Asn1Type>
							<Asn1Type name="Null" tag="5">
								<Blob />
							</Asn1Type>
						</Block>
					</Asn1Type>
				</Asn1Type>
			</Block>
		</Asn1Type>
	</DataModel>

	<!-- Simple Network Management Protocol: GetNextRequest data model... -->
	<DataModel name="SnmpNext" ref="SNMPHeaderV2">
		<Asn1Type name="SnmpAll.Value.Request" class="2" pc="1" tag="1">
			<Block name="Value">
				<Asn1Type name="RequestId" tag="2">
					<Number name="Value" size="32" signed="false" value="0x72a020a6"/>
				</Asn1Type>
				<Asn1Type name="ErrorStatus" tag="2">
					<Number name="Value" size="8" signed="false" value="0"/>
				</Asn1Type>
				<Asn1Type name="ErrorIndex" tag="2">
					<Number name="Value" size="8" signed="false" value="0"/>
				</Asn1Type>

				<Asn1Type name="VariableBindings" pc="1" tag="16">
					<Asn1Type name="Value" pc="1" tag="16">
						<Block name="Value">
							<Asn1Type name="OID" tag="6">
								<!-- 1.3.6.1.2.1.25.1.7.0 -->
								<Blob valueType="hex" value="2b 06 01 02 01 19 01 07 00"/>
							</Asn1Type>
							<Asn1Type name="Null" tag="5">
								<Blob />
							</Asn1Type>
						</Block>
					</Asn1Type>
				</Asn1Type>
			</Block>
		</Asn1Type>
	</DataModel>

	<!-- Simple Network Management Protocol: GetBulkRequest data model... -->
	<DataModel name="SnmpBulk" ref="SNMPHeaderV2">
		<Asn1Type name="SnmpAll.Value.Request" class="2" pc="1" tag="5">
			<Block name="Value">
				<Asn1Type name="RequestId" tag="2">
					<Number name="Value" size="32" signed="false" value="0x72a020a6"/>
				</Asn1Type>
				<Asn1Type name="NonRepeaters" tag="2">
					<Number name="Value" size="8" signed="false" value="0"/>
				</Asn1Type>
				<Asn1Type name="MaxRepetitions" tag="2">
					<Number name="Value" size="8" signed="false" value="50"/>
				</Asn1Type>

				<Asn1Type name="VariableBindings" pc="1" tag="16">
					<Asn1Type name="Value" pc="1" tag="16">
						<Block name="Value">
							<Asn1Type name="OID" tag="6">
								<!-- Object Name: 1.3.6.1.2.1.1.4.0 (iso.3.6.1.2.1.1.4.0) -->
								<!--<Blob name="SnmpName" valueType="hex" value="2b 06 01 02 01 19 01 07 00"/>-->
								<Blob name="SnmpName" valueType="hex" value="2b 06 01"/>
							</Asn1Type>
							<Asn1Type name="Null" tag="5">
								<Blob />
							</Asn1Type>
						</Block>
					</Asn1Type>
				</Asn1Type>
			</Block>
		</Asn1Type>
	</DataModel>

