<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			 xsi:schemaLocation="http://peachfuzzer.com/2012/Peach /peach/peach.xsd"  
       author="Deja Vu Security, LLC" description="Office PIT DataModels" version="0.0.1">
	<!-- Copyright (c) Deja Vu Security, LLC 2013 -->

	<Include ns="PNG" src="file:##Path##/_Common/Models/Image/PNG_Data.xml"/>

	<DataModel name='DefaultXmlModel'>
		<String type='utf8'>
			<Analyzer class='Xml'/>
		</String>
	</DataModel>

	<DataModel name='Office'>
		<Blob name='data'>
			<Analyzer class='Zip'>
				<Param name='Map' value='/\.png$/States:DataModels:PNG:File/,/\.xml$/States:DataModels:DefaultXmlModel/,/\.xml\.rels$/States:DataModels:DefaultXmlModel/' />
			</Analyzer>
		</Blob>
	</DataModel>

</Peach>