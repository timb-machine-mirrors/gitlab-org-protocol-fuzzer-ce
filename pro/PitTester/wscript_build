#!/usr/bin/env python

deps = [
	'Peach.Core.dll',
	'Peach.Pro.dll',
	'System.dll',
	'System.Core.dll',
	'System.Xml.dll',
]

pkgs = [
	'NLog',
	'NUnit',
]

bld(
	name = 'Peach.Pro.PitTester.dll',
	features = 'cs paket',
	use = deps,
	use_packages = pkgs,
	paket_settings = dict(
		frameworks = ['net45'],
	),
	source = bld.path.ant_glob('**/*.cs'),
)
