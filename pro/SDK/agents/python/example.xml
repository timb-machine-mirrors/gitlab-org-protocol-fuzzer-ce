<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://peachfuzzer.com/2012/Peach /peach/peach.xsd">

	<StateModel name="TheState" initialState="Initial">
		<State name="Initial">
			
			<Action type="output">
				<DataModel name="TheDataModel">
					<String value="Output data" />
				</DataModel>
			</Action>

			<Action type="input">
				<DataModel name="TheDataModel">
					<String />
				</DataModel>
			</Action>

			<Action type="call" method="ScoobySnacks">
				<Param name="P1">
					<DataModel name="Foo">
						<String value="Parameter 1" />
					</DataModel>
				</Param>
				<Param name="P2">
					<DataModel name="Foo">
						<String value="Parameter 2" />
					</DataModel>
				</Param>
			</Action>
		</State>

	</StateModel>

	<Agent name="TheAgent" location="http://10.0.1.201:8080">
 		<Monitor class="windowsdebugger">
			<Param name="commandline" value="mspaint.exe fuzzed.png" />
			<Param name="windbgpath" value="c:\program files (x86)\debugging tools for windows (x86)" />
			<Param name="startoncall" value="scoobysnacks"/>
		</Monitor>
		<Monitor class="pageheap">
			<Param name="executable" value="mspaint.exe"/>
			<Param name="windbgpath" value="c:\program files (x86)\debugging tools for windows (x86)" />
		</Monitor>
 	</Agent>

	<Test name="Default">
		<Agent ref="TheAgent"/>
		<StateModel ref="TheState"/>
		 <Publisher class="Remote">
                <Param name="Agent" value="TheAgent" />
                <Param name="Class" value="PublisherInAgent"/>

                <!-- Parameters -->
                <Param name="Host" value="127.0.0.1" />
                <Param name="Port" value="8080" />
        </Publisher>
	</Test>

</Peach>
<!-- end -->
