<?xml version="1.0" encoding="utf-8"?>
<PitDefines>
  <Windows platform="windows">
    <Define key="Arguments" name="" description="" type="" value="telnet://##SourceIPv4##:##SourcePort##"/>
    <Define key="Interface" name="" description="" type="" value="Local Area Connection"/>
    <Define key="Program" name="" description="" type="" value="..\..\..\..\Downloads\putty.exe"/>
    <Define key="SourceIPv4" name="" description="" type="" value="10.0.1.82"/>
  </Windows>

  <Linux platform="linux">
    <Define key="Arguments" name="" description="" type="" value="##SourceIPv4## ##SourcePort##"/>
    <Define key="Interface" name="" description="" type="" value="eth0"/>
    <Define key="Program" name="" description="" type="" value="telnet"/>
    <Define key="SourceIPv4" name="" description="" type="" value="10.0.1.92"/>
  </Linux>

  <OSX platform="osx">
    <Define key="Arguments" name="" description="" type="" value="##SourceIPv4## ##SourcePort##"/>
    <Define key="Interface" name="" description="" type="" value="en0"/>
    <Define key="Program" name="" description="" type="" value="telnet"/>
    <Define key="SourceIPv4" name="" description="" type="" value="10.0.1.94"/>
  </OSX>

  <All>
    <Define key="TargetPort" name="" description="" type="" value="23"/>
    <Define key="SourcePort" name="" description="" type="" value="31337"/>
    <Define key="TargetIPv4" name="" description="" type="" value="10.0.1.23"/>
    <Define key="LoggerPath" name="" description="" type="" value="./logs/telnet/"/>
    <Define key="Strategy" name="" description="" type="" value="Random"/>
    <Define key="PitLibraryPath" name="" description="" type="" value="."/>
  </All>

</PitDefines>
