<?xml version="1.0" encoding="utf-8"?>
<PitDefines>
	<All>
		<String key="SerialPort"
				value="COM1"
				name="Serial port device name."
				description=""/>
		
		<Range  key="Baudrate"
				value="57600"
				min="0" max="2000000"
				name="Baud Rate"
				description="The serial port baud rate. Supported baud rates: 110
300, 600, 1200, 2400, 4800, 9600, 14400, 19200, 28800, 38400, 56000, 57600, 115200."/>
		
		<Enum   key="Parity"
				value="None"
				enumType="System.IO.Ports.Parity"
				name="Parity"
				description="Specifies the parity bit."/>
		
		<Range  key="DataBits"
				value="8"
				min="5" max="8"
				name="Data Bits"
				description="The data bits value."/>
		
		<Enum   key="StopBits"
				value="One"
				enumType="System.IO.Ports.StopBits"
				name="Stop Bits"
				description="Specifies the number of stop bits used."/>
		
		<Enum   key="Handshake"
				value="XOnXOff"
				enumType="System.IO.Ports.Handshake"
				name="Handshake"
				description="Specifies the control protocol used in establishing a serial port communication."/>
		
		<Range  key="Timeout"
				value="5000"
				min="0" max="10000000"
				name="Timeout"
				description="Timeout in milliseconds to wait for data to be send or received. During fuzzing a timeout failure will cause the fuzzer to skip to the next iteration."/>

		<String key="LoggerPath"
				value="logs/modus_ascii_serial/"
				name="Logger Path"
				description="The directory where Peach will save the log produced when fuzzing." />

		<Strategy key="Strategy"
				  value="Random"
				  name="Mutation Strategy"
				  description="The mutation strategy to use when fuzzing." />

		<String key="PitLibraryPath"
				value="."
				name="Pit Library Path"
				description="The path to the root of the pit library."/>
	</All>
</PitDefines>
