<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://peachfuzzer.com/2012/Peach ../peach.xsd"
       author="Deja Vu Security, LLC" description="Virtual LAN PIT " version="0.0.2">
	<!-- Copyright (c) Deja Vu Security, LLC 2014 -->
	<!-- Requires Peach 3.0 and RawEther publisher -->
	
	<Include ns="States" src="file:##PitLibraryPath##/_Common/Models/Net/VLAN_State.xml"/>

	<Agent name="LocalAgent">
		<Monitor class="Pcap">
			<Param name="Device" value="##Interface##" />
		</Monitor>
	</Agent>

	<Test name="Default">
		<StateModel ref="States:TheState"/>
		
		<Agent ref="LocalAgent"/>
		
		<Publisher class="RawEther">
			<Param name="Interface" value="##Interface##"/>
			<Param name="Protocol" value="ETH_P_8021Q"/>
		</Publisher>
		
		<Strategy class="##Strategy##"/>
		
		<Logger class="File">
			<Param name="Path" value="##LoggerPath##"/>
		</Logger>
	</Test>
</Peach>
