<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <appSettings>
    <add key="RemoteWebSite" value="http://localhost:8888/p/" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <httpProtocol>
      <redirectHeaders>
        <add name="Accept" value="application/json"/>
      </redirectHeaders>
      <customHeaders>
        <add name="Access-Control-Allow-Origin" value="*"/>
      </customHeaders>
    </httpProtocol>

    <caching enabled="false" />
    <staticContent>
      <mimeMap fileExtension=".json" mimeType="application/json" />
      <clientCache cacheControlMode="DisableCache"/>
    </staticContent>

    
    <handlers>
      <add name="ReverseProxy" verb="*"
           path="/p/*"
           type="Peach.Pro.Client.ReverseProxy, Peach.Pro.Client"
           resourceType="Unspecified" />
    </handlers>
    
    <!--
    <rewrite>
      <rules>
        <rule name="VisualStudio" stopProcessing="true">
          <match url="^p/(.*)"/>
          <action type="Rewrite" redirectType="Temporary" url="http://localhost:8888/p/{R:1}" />
        </rule>
      </rules>
    </rewrite>
    -->

  </system.webServer>
</configuration>