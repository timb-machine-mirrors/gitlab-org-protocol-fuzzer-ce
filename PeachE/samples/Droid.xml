<?xml version="1.0" encoding="utf-8"?>
<Peach xmlns="http://peachfuzzer.com/2012/Peach" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://peachfuzzer.com/2012/Peach ../peach.xsd">


	<DataModel name="TheDataModel">
		<Number size='32' signed="false" value="31337" />
	</DataModel>

	<DataModel name="X">
		<Number size="10" mutable="false" value="100" />
	</DataModel>	

	<DataModel name="Y">
		<Number size="10" mutable="true" value="0" />
	</DataModel>

	<DataModel name="Enter">
		<Number size="16" mutable="false" value="66" />
	</DataModel>

	<DataModel name="Url">
		<String value="http://google.com/m?hl=en&amp;source=android-browser-type&amp;q=foo">
			<Analyzer class="StringToken"/>
		</String>
	</DataModel>

	<StateModel name="State" initialState="State1" >
		<State name="State1"  >
			<!--Action type="call" method="monkey" >
				<Param type="in">
					<DataModel ref="TheDataModel"/>
				</Param>
			</Action-->
			<Action type="setProperty" property="x">
				<DataModel ref="X" />
			</Action>
			<Action type="setProperty" property="y">
				<DataModel ref="Y" />
			</Action>
			<Action type="call" method="tap" />
		</State>
	</StateModel>

	<StateModel name="BrowserState" initialState="State1" >
		<State name="State1"  >
			<Action type="call" method="text">
				<Param>
					<DataModel ref="Url"/>
				</Param>
			</Action>

			<Action type="setProperty" property="keycode">
				<DataModel ref="Enter" />
			</Action>

			<Action type="call" method="keyevent"/>
		</State>
	</StateModel>

	<Agent name="Ag">
		<Monitor class="Android">
			<!--Param name="CmdOnStart" value="am start -S -n com.android.development/.BadBehaviorActivity &amp;&amp; sleep 2" /-->
			<Param name="ApplicationName" value="com.android.development" />
			<Param name="ActivityName" value=".BadBehaviorActivity" />
			<!--Param name="AdbPath" value="/path/to/adb"/-->
		</Monitor>
	</Agent>

	<Agent name="Browser">
		<Monitor class="Android">
			<Param name="ApplicationName" value="com.android.browser" />
			<Param name="ActivityName" value=".BrowserActivity" />
			<!--Param name="AdbPath" value="/path/to/adb"/-->
		</Monitor>
	</Agent>

	<Test name="Default">
		<StateModel ref="State"/>
		<Agent ref="Ag" />

		<!--Publisher class="Null" /-->
		<Publisher class="AndroidMonkey">
			<Param name="Target" value="com.android.development" />
			<Param name="DeviceSerial" value="##Serial##" />
			<Param name="NumActions" value="20" />
			<Param name="Sleep" value="0" />
		</Publisher>
		<Logger class="File">
			<Param name="Path" value="logs"/>
		</Logger>
	</Test>

	<Test name="Browser">
		<StateModel ref="BrowserState"/>
		<Agent ref="Browser" />

		<!--Publisher class="Null" /-->
		<Publisher class="AndroidMonkey">
			<Param name="Target" value="com.android.browser" />
			<Param name="DeviceSerial" value="##Serial##" />
			<Param name="NumActions" value="20" />
			<Param name="Sleep" value="0" />
		</Publisher>
		<Logger class="File">
			<Param name="Path" value="logs"/>
		</Logger>
	</Test>

</Peach>
<!-- end -->
